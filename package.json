{
  "name": "next-playground",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=18",
    "pnpm": ">=7",
    "yarn": "please-use-pnpm",
    "npm": "please-use-pnpm"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "generate:icons": "rimraf src/generated/icons && svgr src/assets/icons --no-dimensions && eslint src/generated/icons --ext .ts,.tsx --fix && prettier --write src/generated/icons",
    "lint": "concurrently \"pnpm:lint:*\" -c magenta,yellow,blue -n lint: --timings",
    "lint:eslint": "eslint . --ext .ts,.tsx,.js --fix --ignore-path .gitignore",
    "lint:prettier": "prettier --write . --ignore-path .gitignore",
    "lint:typescript": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/server": "11.10.0",
    "@hookform/resolvers": "2.9.10",
    "@mantine/core": "5.10.0",
    "@mantine/hooks": "5.10.0",
    "@mantine/labs": "5.10.0",
    "@mantine/next": "5.10.0",
    "@next/font": "13.1.1",
    "@prisma/client": "4.8.1",
    "@tabler/icons": "1.119.0",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.41.5",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@next/eslint-plugin-next": "13.1.1",
    "@svgr/cli": "6.5.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.2.5",
    "@types/jest-axe": "3.5.5",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "concurrently": "7.6.0",
    "eslint": "8.30.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-file-progress": "1.3.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.2.0",
    "eslint-plugin-jest-dom": "4.0.3",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-sonarjs": "0.17.0",
    "eslint-plugin-testing-library": "5.9.1",
    "eslint-plugin-unicorn": "45.0.2",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "jest-axe": "7.0.0",
    "jest-environment-jsdom": "29.3.1",
    "jest-extended": "3.2.0",
    "lint-staged": "13.1.0",
    "prettier": "2.8.1",
    "prisma": "4.8.1",
